{{template "base" .}}

{{define "title"}}Notes{{end}}

{{define "body"}}

{{range .Notes}}

<div class="card" style="width: 18rem;">
  <img src="..." class="card-img-top" alt="{{.Title}}">
  <div class="card-body">
    <h5 class="card-title">{{.Title}}</h5>
    <p class="card-text">{{.Text}}</p>
    <a href="#" class="btn btn-primary">Delete Note</a>
  </div>
</div>

{{else}}
<p>No Notes!</p>
{{end}}

{{end}}
